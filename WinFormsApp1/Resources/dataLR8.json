{
  "matrix": 
  {
    "type": { "type": ">", "var": "<", ";": "<", "=": "<", "c": "<", "t": "<", "a": "<", "d": "<", "union": "<", "end": "<" },
    "var": { "type": ">", ":": "<", "var": "<", ";": "<", "=": "<", "c": "<", "t": "<", "a": "<", "d": "<", "union": "<", "end": ">" },
    ";": { "type": ">", "var": ">", ";": "<", "=": ">", "c": ">", "t": ">", "a": "<", "d": "-", "union": ">", "end": ">", ":":"<"  },
    "=": { "type": "<", "var": "<", ";": "<", "=": "-", "c": "<", "t": "<", "a": "<", "d": "<", "union": "<", "end": "<" },
    "c": { "type": ">", "var": ">", ";": ">", "=": ">", "c": "-", "t": ">", "a": ">", "d": "-", "union": ">", "end": ">" },
    "t": { "type": ">", "var": ">", ";": ">", "=": ">", "c": "-", "t": ">", "a": ">", "d": "-", "union": ">", "end": ">" },
    "a": { "type": ">", ":": ">", "var": ">", ";": "-", "=": ">", "c": "-", "t": ">", "a": ">", "d": "-", "union": ">", "end": ">" },
    "d": { "type": ">", "var": ">", ";": "-", "=": ">", "c": "-", "t": ">", "a": ">", "d": "-", "union": ">", "end": ">" },
    "union": { "type": ">", "var": ">", ";": "<", "=": "<", "c": "<", "t": "<", "a": "<", "d": "<", "union": ">", "end": ">" },
    "end": { "type": ">", "var": ">", ";": "-", "=": "-", "c": ">", "t": ">", "a": ">", "d": ">", "union": ">", "end": ">" },
    "start": { "type": "<", "end": "<", "var": "<" },
    ":": {"c": "<", "t" :  "<", ";": ">"}
  },

  "rules": {
    "S": ["type", "L", "var", "R"],
    "L": [ "T", ";", "|", "T", ";", "L"],
    "T": ["t","=", "c", "|", "t", "=", "D"],
    "R": ["V", ";", "|", "V", ";", "R"],
    "V": ["K", ":", "t", "|", "K", ":", "c", "|","K", ":", "D"],
    "K": [ "K", ",", "a", "|", "a" ],
    "D": ["union", "F", "end"],
    "F": ["E", ";", "|", "E", ";", "F"],
    "E": ["K", ":", "c", "|","K", ":", "t"]
  }
}


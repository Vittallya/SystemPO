    "or": {"a": "<", ";": ">","or": ">", "xor": ">", "and": "<", "(": "<", ")": ">", "not": "<"},
    "xor": {"a": "<", ";": ">","or": ">", "xor": ">", "and": "<", "(": "<", ")": ">", "not": "<"},
    "and": {"a": "<", ";": ">","or": ">", "xor": ">", "and": "<", "(": "<", ")": ">", "not": "<"},
    "(": {"a" :  "<", "or": "<", "xor": "<", "and": "<", "(": "<", ")": "=", "not": "<"},
    ")": {";": ">", "or": ">", "xor": ">", "and": ">", ")": ">"},
    "not": {"(": "="},
    ":=": {"a" :  "<",";": "=", "or": "<", "xor" : "<", "and": "<", "(": "<", "not": "<"},
